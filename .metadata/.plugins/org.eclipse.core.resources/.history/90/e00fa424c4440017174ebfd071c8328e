package bd.dbos;

import java.util.Vector;

public class TabelaJogos {
	private Vector <Jogo> jogos;
	
	public TabelaJogos(){}
	
	public void pushJogo(Jogo jg){
		this.jogos.add(jg);
	}
	public String toString(){
		String retorno = "";
		for(int i =0; i<this.jogos.size();i++){
			retorno += 
					"Nome Ganhador :" + this.jogos.get(i).getUser().getNome() +
					"Data de Termino do jogo: "+this.jogos.get(i).getDataTermino().toString();
		}
		return retorno;
	}

}
